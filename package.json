{
  "name": "@vuesax-alpha/auto-import-resolver",
  "type": "module",
  "version": "1.0.1",
  "description": "On-demand components auto importing for Vuesax Alpha.",
  "author": "tranthinh-coding",
  "license": "MIT",
  "homepage": "https://github.com/vuesax-alpha/auto-import-resolver#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesax-alpha/auto-import-resolver.git"
  },
  "bugs": "https://github.com/vuesax-alpha/auto-import-resolver/issues",
  "keywords": [
    "vuesax-alpha",
    "vuesax alpha",
    "auto import",
    "vue",
    "vuesax"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "release": "bumpp"
  },
  "dependencies": {
    "local-pkg": "^0.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.42.0",
    "bumpp": "^9.3.0",
    "eslint": "8.49.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  }
}
